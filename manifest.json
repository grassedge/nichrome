{
    "name": "nichrome",
    "description": "2ch browser",
    "version": "0.0.1.2",
    "manifest_version": 2,
    "app": {
        "launch": {
            "local_path": "index.html"
        }
    },
    "icons": {
        "16" : "public/img/icon_16.png",
        "128": "public/img/icon_128.png"
    },
    "permissions": [
        "http://*/*",
        "unlimitedStorage",
        "notifications",
        "geolocation",
        "background"
    ]
}
